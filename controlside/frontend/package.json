{
  "name": "reactify-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ag-grid-community/client-side-row-model": "^23.1.1",
    "@ag-grid-community/core": "^23.1.1",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "ace-builds": "^1.4.12",
    "ag-grid-community": "^23.1.1",
    "ag-grid-react": "^23.1.1",
    "bootstrap": "^4.5.0",
    "brace": "^0.11.1",
    "copy-to-clipboard": "^3.3.1",
    "jquery": "^3.5.1",
    "jsoneditor": "^9.0.3",
    "jsoneditor-react": "^3.0.0",
    "popper.js": "^1.16.1",
    "react": "^16.8.0",
    "react-ace": "^9.1.2",
    "react-bootstrap": "^1.0.1",
    "react-bootstrap-icons": "^1.0.3-alpha5",
    "react-cookies": "^0.1.1",
    "react-dom": "^16.8.0",
    "react-json-pretty": "^2.2.0",
    "react-router-dom": "^4.2.2",
    "react-select": "^3.1.0",
    "react-sidebar": "^3.0.2",
    "save": "^2.4.0",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "react-scripts": "1.1.4",
    "copyfiles": "^2.0.0",
    "renamer": "^0.7.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "collect": "react-scripts build && npm run transfer",
    "transfer": "npm run build-rename && npm run copy-buildfiles",
    "build-rename": "npm run build-rename-js && npm run build-rename-css && npm run rename-service-js",
    "build-rename-logo": "renamer --regex --find '([^\\.])\\.[^\\.]+\\.(\\w+)' --replace '$1.$2' build/static/media/*",
    "build-rename-js": "renamer --regex --find 'main\\.[^\\.]+\\.js' --replace 'react.ui.js' build/static/js/*.js",
    "build-rename-css": "renamer --regex --find 'main\\.[^\\.]+\\.css' --replace 'react.ui.css' build/static/css/*.css",
    "rename-service-js": "npm run rename-in-css && npm run rename-in-js",
    "rename-in-css": "sed -i 's/main\\..*\\.css/react.ui.css/g' build/service-worker.js",
    "rename-in-js": "sed -i 's/main\\..*\\.js/react.ui.js/g' build/service-worker.js",
    "copy-buildfiles": "npm run copy-build-js && npm run copy-build-css && npm run copy-build-img && npm run copy-build-media && npm run copy-service-worker",
    "clear-static": "npm run clear-js && npm run clear-css",
    "clear-js": "rm -f ../staticfiles/js/*",
    "clear-css": "rm -f ../staticfiles/css/*",
    "copy-build-img": "copyfiles -f 'build/static/img/*' '../fleet_manager/staticfiles/img/'",
    "copy-build-media": "copyfiles -f 'build/static/media/*' '../fleet_manager/staticfiles/media/'",
    "copy-build-js": "copyfiles -f 'build/static/js/*' '../fleet_manager/staticfiles/js/'",
    "copy-build-css": "copyfiles -f 'build/static/css/*' '../fleet_manager/staticfiles/css/'",
    "copy-service-worker": "copyfiles -f 'build/service-worker.js' '../fleet_manager/staticfiles/js/'",
    "collectstatic": "python3 ../manage.py collectstatic --no-input"
  }
}
